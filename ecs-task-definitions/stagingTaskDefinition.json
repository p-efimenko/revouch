[
  {
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/staging-revouch-web",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "ecs"
      }
    },
    "portMappings": [
      {
        "hostPort": 3000,
        "protocol": "tcp",
        "containerPort": 3000
      }
    ],
    "environment": [
      { "name": "NEXTAUTH_URL",
        "value": "https://staging-revouch.artkai.dev"
      }
    ],
    "secrets": [
      { "name": "NEXTAUTH_SECRET",
        "valueFrom": "arn:aws:secretsmanager:us-east-1:280348121942:secret:staging-revouch-secret-0dauZ3"
      }
     ],
    "memory": 1024,
    "cpu": 512,
    "stopTimeout": 5,
    "image": "280348121942.dkr.ecr.us-east-1.amazonaws.com/staging-revouch-web:latest",
    "startTimeout": 60,
    "essential": true,
    "readonlyRootFilesystem": false,
    "name": "staging-revouch-web"
  }
]