[
  {
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/dev-revouch-web",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "ecs"
      }
    },
    "portMappings": [
      {
        "hostPort": 3000,
        "protocol": "tcp",
        "containerPort": 3000
      }
    ],
    "environment": [
      { "name": "NEXTAUTH_URL",
        "value": "https://dev-revouch.artkai.dev"
      },
      { "name": "NEXT_PUBLIC_API_URL",
        "value": "https://dev-revouch.artkai.dev/api/v1"
      }
    ],
    "secrets": [
      { "name": "NEXTAUTH_SECRET",
        "valueFrom": "arn:aws:secretsmanager:us-east-1:101257774107:secret:dev-revouch-secret-uCTPj0"
      }
     ],
    "memory": 1024,
    "cpu": 512,
    "stopTimeout": 5,
    "image": "101257774107.dkr.ecr.us-east-1.amazonaws.com/dev-revouch-web:latest",
    "startTimeout": 60,
    "essential": true,
    "readonlyRootFilesystem": false,
    "name": "dev-revouch-web"
  }
